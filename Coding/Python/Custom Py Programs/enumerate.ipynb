     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0 apple\n",
      "1 banana\n",
      "2 mango\n"
     ]
    }
   ],
   "source": [
    "fruits = [\"apple\", \"banana\", \"mango\"]\n",
    "\n",
    "for i, fruit in my_enumerate(fruits):\n",
    "    print(f\"{i} {fruit}\")\n"
   ]
  }
