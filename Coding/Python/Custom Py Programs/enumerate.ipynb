   "source": [
    "def my_enumerate(iterable, start=0):\n",
    "    index = start\n",
    "    for item in iterable:\n",
    "        yield index, item\n",
    "        index += 1\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "e3157568",
   "metadata": {},
   "outputs": [
